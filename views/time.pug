html
	head
		title= 'Delta time'
		link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css', integrity='sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u', crossorigin='anonymous')
		style
			include style.css
	body
		ul.menu1
			li.menu2
				a.active(href='/delta') Cклад Дельты
			li.menu2
				a(href='#news') News
			li.menu2
				a(href='#contact') Contact
			li.menu2
				a(href='#about') About
		h1= 'Склад Дельта'
	table
		tr
			th="id"
			th="sku"
			th="Последнее количество"
			th="Крайняя цена Прайс"
			th="Наличие на складе"
		for delta in deltaList
			tr
				td(class='id')= delta.id
				td
					a(href='/delta/'+delta.sku) #{delta.sku}
				td(class='quant')= delta.quantity
				td(class='price')= delta.price
				td
					a(href='/delta/time/'+delta.time) #{delta.time}